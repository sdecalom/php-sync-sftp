{
    "name": "sdecalom/sync-sftp",
    "type": "library",
    "description": "Sync local files with ftp server",
    "keywords": ["sync", "ftp", "file"],
    "homepage": "https://github.com/sdecalom/php-sync-ftp",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastien",
            "homepage": "http://dev.glicer.com"
        },
        {
            "name": "Emmanuel ROECKER",
            "homepage": "http://dev.glicer.com"
        },
        {
            "name": "Rym BOUCHAGOUR",
            "homepage": "http://dev.glicer.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/finder": "~2.3 || ~3.0",
        "phpseclib/phpseclib": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "GlSyncFtp\\": "src/"
        }
